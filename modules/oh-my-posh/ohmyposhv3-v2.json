          "powerline_symbol": "ÓÇ∞",
          "properties": {
            "time_format": "Mon 3:04PM"
          },
          "style": "powerline",
          "template": " {{ .CurrentDate | date .Format }} ",
          "type": "time"
        },
        {
          "type": "session",
          "powerline_symbol": "ÓÇ∞",
          "style": "powerline",
          "foreground": "#ffffff",
          "background": "#c386f1",
          "template": "{{ if .SSHSession }}ÓÆ© {{ .UserName }}@{{ .HostName }}{{else}}{{ .UserName }}{{ end }}"
        },
        {
          "type": "project",
          "style": "powerline",
          "powerline_symbol": "ÓÇ∞",
          "foreground": "#193549",
          "background": "#ffeb3b",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ if .Version }}Ôíá {{.Version}}{{ end }} {{ if .Name }}{{ .Name }}{{ end }}{{ end }} "
        },
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "ÓÇ∞",
          "foreground": "#ffffff",
          "background": "#61AFEF",
          "properties": {
            "style": "folder",
            "mapped_locations": {
              "~/dotfiles": "‚öôÔ∏è"
            }
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "ÓÇ∞",
          "foreground": "#193549",
          "background": "#ffeb3b",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FFEB3B{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#FFCC80{{ end }}",
            "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
            "{{ if gt .Behind 0 }}#B388FB{{ end }}"
          ],
          "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} üíæ {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} üìñ {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} üôà {{ .StashCount }}{{ end }}",
          "properties": {
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "untracked_modes": {
              "/Users/user/Projects/oh-my-posh/": "no"
            },
            "source": "cli",
            "mapped_branches": {
              "main": "üî•",
              "feat/*": "üöÄ",
              "feature/*": "üöÄ",
              "bug/*": "üêû"
            }
          }
        },
        {
          "type": "kubectl",
          "style": "powerline",
          "powerline_symbol": "ÓÇ∞",
          "foreground": "#000000",
          "background": "#ebcc34",
          "template": " Ô¥± {{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}} ",
          "properties": {
            "context_aliases": {
              "arn:aws:eks:eu-west-1:1234567890:cluster/posh": "posh"
            }
          }
        },
        {
          "type": "npm",
          "style": "powerline",
          "powerline_symbol": "ÓÇ∞",
          "foreground": "#193549",
          "background": "#ffeb3b",
          "template": "Óúû {{ .Full }} "
        },
        {
          "type": "pnpm",
          "style": "powerline",
          "powerline_symbol": "ÓÇ∞",
          "foreground": "#000000",
          "background": "#F9AD00",
          "template": " Û∞ãÅ {{ .Full }} "
        },
        {
          "type": "react",
          "style": "powerline",
          "powerline_symbol": "ÓÇ∞",
          "foreground": "#81a1c1",
          "template": " Óû∫ {{ .Full }} "
        },
        {
