          "properties": {
            "api_key": "0fcecd73687a62ded000b3a015c4dec5",
            "location": "Hoppers Crossing,AU",
            "units": "metric",
            "http_timeout": 1000,
            "cache_timeout": 30
          }
        }
      ]
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "sysinfo",
          "powerline_symbol": "ÓÇ∞",
          "foreground": "#ffffff",
          "background": "#ED4337",
