            "api_url": "http://localhost:26538"
          },
          "template": "{{ .Icon }}{{ if ne .Status \"stopped\" }}{{ .Artist }}-{{ .Track }}{{ end }}"
        },
        {
          "type": "owm",
          "style": "powerline",
          "powerline_symbol": "ÓÇ∞",
          "foreground": "#ffffff",
          "background": "#FF0000",
